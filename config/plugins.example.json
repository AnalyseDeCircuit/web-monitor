{
  "containers": {
    "webshell": {
      "containerName": "web-monitor-plugin-webshell",
      "baseUrl": "http://127.0.0.1:38101"
    },
    "filemanager": {
      "containerName": "web-monitor-plugin-filemanager",
      "baseUrl": "http://127.0.0.1:38102"
    }
  },
  "ssh": {
    "host": "127.0.0.1",
    "port": 22,
    "user": "webmonitor",
    "knownHosts": "/etc/webmonitor/known_hosts",
    "insecureHostKey": false,
    "connectTimeout": "10s",
    "keys": {
      "webshell": "/etc/webmonitor/webshell_id_ed25519",
      "filemanager": "/etc/webmonitor/filemanager_id_ed25519"
    }
  },
  "webshell": {
    "idleTimeout": "10m"
  },
  "filemanager": {
    "allowedPrefixes": ["/var/log"],
    "writeEnabled": false
  }
}
